<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" clear_params="false" output="screen">  
    <!--output="screen"-->
    <rosparam file="$(find braveheart_exploring)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find braveheart_exploring)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find braveheart_exploring)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find braveheart_exploring)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find braveheart_exploring)/config/base_local_planner_params.yaml" command="load" />
    
    <!--param name="base_global_planner" value="global_planner/GlobalPlanner"/>
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/-->
  </node>
</launch>
