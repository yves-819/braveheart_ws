<launch>

    <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >

		<param name="video_device" value="/dev/usb_cam1" />
		<param name="image_width" value="640" />
		<param name="image_height" value="480" />
		<param name="pixel_format" value="yuyv" />
		<param name="camera_frame_id" value="usb_cam" />
		<param name="io_method" value="mmap"/>

        <param name="camera_info_url" type="string" value="file://$(find robot_vision)/ost1.yaml" />
    </node>

	<node name="usb_cam_tf" pkg="tf" type="static_transform_publisher" args="0.16 -0.11 0.27 0 0 0 /base_link /usb_cam 20"/>

	<node name="usb_cam2" pkg="usb_cam" type="usb_cam_node" output="screen" >

		<param name="video_device" value="/dev/usb_cam2" />
		<param name="image_width" value="640" />
		<param name="image_height" value="480" />
		<param name="pixel_format" value="yuyv" />
		<param name="camera_frame_id" value="usb_cam2" />
		<param name="io_method" value="mmap"/>

        <!--param name="camera_info_url" type="string" value="file://$(find robot_vision)/ost2.yaml" /-->
    </node>
	<node name="usb_cam_tf2" pkg="tf" type="static_transform_publisher" args="0.16 -0.11 0.27 0 0 0 /base_link /usb_cam2 20"/>
	

</launch>
